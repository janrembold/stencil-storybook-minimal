{
  "name": "stencil-storybook-minimal",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-storybook-minimal/stencil-storybook-minimal.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --watch",
    "build.storybook": "build-storybook",
    "start": "npm-run-all --parallel start.*",
    "start.stencil": "stencil build --watch --docs --no-open",
    "start.storybook": "start-storybook -p 6006",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "2.5.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/plugin-syntax-jsx": "7.12.13",
    "@babel/plugin-transform-react-jsx": "7.13.12",
    "@pxtrn/storybook-addon-docs-stencil": "1.0.0",
    "@storybook/addon-actions": "6.2.5",
    "@storybook/addon-essentials": "6.2.5",
    "@storybook/html": "6.2.5",
    "babel-loader": "8.2.2",
    "babel-plugin-jsx-pragmatic": "1.0.2",
    "jsx-dom": "7.0.0-beta.4",
    "npm-run-all": "4.1.5"
  }
}
